{
  "extends": "@react-native/typescript-config",
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": [
    "**/node_modules",
    "**/Pods",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "compilerOptions": {
    "types": ["jest"],
    "baseUrl": "./",
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true
  }
}
